## GENERAL ##
sampling_rate: 96000;
#filter_length: 65536;
filter_length: 16384,4;
overflow_warnings: true;
show_progress: true;
max_dither_table_size: 0;
allow_poll_mode: false;
powersave: false;
monitor_rate: false;
lock_memory: true;
sdf_length: -1;
logic: "cli" { port: 3000; };

