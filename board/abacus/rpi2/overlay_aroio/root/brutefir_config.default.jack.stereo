## FILTERS ##
## L
filter "left" {
        from_inputs: "left";
####### Do NOT change the following line by a char!!!
        to_outputs:"left"/0/1;
	process: -1;
####### Do NOT change the following line by a char!!!
	coeff:"left0";
};

## R
filter "right" {
        from_inputs: "right";
####### Do NOT change the following line by a char!!!
        to_outputs:"right"/0/1;
	process: -1;
####### Do NOT change the following line by a char!!!
	coeff:"right0";
};
