## INPUTS / OUTPUTS ##
input "left", "right" {
        device: "jack" { ports: "" , "" ; };
#        device: "alsa" { device: "aduplex" ; ignore_xrun:true; };
#	device: "file" { path: "/tmp/test.pcm"; };
	sample: "AUTO";
#        sample: "S32_LE";
        channels: 2;
};

output "left", "right" {
        device: "jack" { ports: "system:playback_1" , "system:playback_2" ; };
	sample: "AUTO";
#        device: "alsa" { device: "hw:0" ; ignore_xrun:true; };
#        sample: "S32_LE";
        channels: 2;
        delay: 0 , 0;
	maxdelay: 8000;
};

